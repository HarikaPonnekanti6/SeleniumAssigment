<?xml version="1.0" encoding="UTF-8"?>
<!-- The below tag runs two instances of the browsers in parallel -->
<suite name="AmazonSuite" thread-count="2" parallel="tests">
	<test name="TestInChrome">
		<parameter name="browserName" value="Chrome" />
		<classes>
			<class name="SeleniumTest.AmazonKindleTest">
			</class>
		</classes>
	</test>
	<test name="TestInFirefox">
		<parameter name="browserName" value="firefox" />
		<classes>
			<class name="SeleniumTest.AmazonKindleTest">
			</class>
		</classes>
	</test>
</suite> 